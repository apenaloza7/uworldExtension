{
    "manifest_version": 3,
    "name": "UWorld Celebration",
    "version": "1.0",
    "description": "Adds celebration effects to UWorld correct answers and test scores!",
    "permissions": ["activeTab"],
    "content_scripts": [{
      "matches": ["*://*.uworld.com/*"],
      "js": ["content.js"]
    }],
    "web_accessible_resources": [{
      "resources": [
        "hooray.mp3", 
        "outroAvg.mp3",
        "observer.js",
        "audio.js",
        "confetti.js"
      ],
      "matches": ["*://*.uworld.com/*"]
    }]
}